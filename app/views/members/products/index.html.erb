<h1>Members_Products_Index</h1>

<div class="container">
	<div class="row">
	<div class="col-xs-3">
		<label>
			ジャンル検索
		</label>
	</div>
	<div class="col-xs-9">
		<% @products.each do |product| %>
			<%= link_to product.name, members_product_path(product) %>
			¥<%= product.unit_price %><br>
		<% end %>
	</div>
	</div>
</div>