<div class="container">
	<div class="row">

      <div class=col-xs-9>
        <h2>会員一覧</h2>
        <table class="table">
          <thead>

            <tr>
              <th>会員ID</th>
              <th>氏名</th>
              <th>メールアドレス</th>
              <th>ステータス</th>
            </tr>
          </thead>

          <tbody>
            <% @members.each do |member| %>
            <tr>
              <td><%= member.id %></td>
              <td><%= link_to member.last_name, admins_members_path, class: "member_#{member.id}" %><%= link_to member.first_name, admins_members_path, class: "member_#{member.id}" %></td>
              <td><%= member.email %></td>
              <td><%= member.is_enabled %></td>
            </tr>
            <% end %>
          </tbody>

        </table>
    </div>
  </div>
</div>